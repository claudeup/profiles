{
  "name": "jupyter-ml",
  "description": "Data science and machine learning with Jupyter notebooks. Includes pandas, scikit-learn, and visualization tooling for data analysis workflows.",
  "plugins": [
    "superpowers@superpowers-marketplace",
    "episodic-memory@superpowers-marketplace"
  ],
  "mcpServers": [],
  "marketplaces": [
    {
      "source": "github",
      "repo": "anthropics/claude-code-plugins"
    },
    {
      "source": "github",
      "repo": "obra/superpowers-marketplace"
    }
  ],
  "detect": {
    "files": ["requirements.txt", "environment.yml"],
    "contains": {
      "requirements.txt": "jupyter",
      "environment.yml": "jupyter"
    }
  }
}
